{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.5.3",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "ln -sfn ../../data/obsidian-vault/Images ./public/static/images && next dev",
    "build": "cp -r data/obsidian-vault/Images public/static/images && next build",
    "postbuild": "yarn run sitemap && yarn run rss",
    "sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
    "rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/inter": "4.5.15",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^2.0.13",
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@vercel/analytics": "^0.1.8",
    "autoprefixer": "^10.4.0",
    "contentlayer": "0.3.0",
    "esbuild": "0.17.4",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.2",
    "mdx-bundler": "^9.2.1",
    "next": "13.1.5",
    "next-contentlayer": "0.3.0",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.21",
    "preact": "^10.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.4.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark": "^14.0.2",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.31.3",
    "tailwindcss": "^3.2.4",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.1.5",
    "@svgr/webpack": "^6.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/react": "^18.0.27",
    "@types/tailwindcss": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.0.0",
    "globby": "13.1.3",
    "husky": "^8.0.3",
    "inquirer": "^9.1.4",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
